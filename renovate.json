{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        "schedule:daily",
        "group:all",
        "config:recommended"
    ],
    "additionalBranchPrefix": "{{baseDir}}-",
    "commitMessagePrefix": "{{baseDir}}",
    "packageRules": [
        {
            "enabled": false,
            "matchFileNames": ["v1.16-4.10/*", "v1.17-5.0/*"]
        }
    ],
    "vulnerabilityAlerts": {
        "enabled": true
    },
    "osvVulnerabilityAlerts": true
}
